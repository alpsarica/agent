{
  "thingId": "ai.composiv.sandbox.f1tenth:composiv_simulator_teb.launch",
  "policyId": "ai.composiv.sandbox.f1tenth:composiv_simulator_teb.launch",
  "definition": "ai.composiv.sandbox.f1tenth:Stack:1.0.0",
  "features": {
    "stack": {
      "properties": {
        "name": "Muto Learning Simulator with TEB",
        "stackId": "ai.composiv.sandbox.f1tenth:composiv_simulator_teb.launch",
        "context": "maze",
        "stack": [
          {
            "thingId": "ai.composiv.sandbox.f1tenth:composiv_simulator.launch"
          }
        ],
        "node": [
          {
            "name": "cmdvel_2_ackermann",
            "pkg": "teb_local_planner_tutorials",
            "exec": "cmd_vel_to_ackermann_drive.py"
          },
          {
            "pkg": "rviz",
            "name": "rviz",
            "exec": "rviz",
            "args": "-d $(find teb_local_planner_tutorials)/cfg/rviz_navigation.rviz",
            "output": "screen"
          },
          {
            "name": "amcl",
            "pkg": "amcl",
            "exec": "amcl",
            "param": [
              {
                "from": "$(find teb_local_planner_tutorials)/cfg/amcl_params.yaml"
              },
              {
                "name": "initial_pose_x",
                "value": "2"
              },
              {
                "name": "initial_pose_y",
                "value": "2"
              },
              {
                "name": "initial_pose_a",
                "value": "0"
              }
            ]
          },
          {
            "name": "move_base",
            "pkg": "move_base",
            "exec": "move_base",
            "param": [
              {
                "from": "$(find teb_local_planner_tutorials)/cfg/carlike/costmap_common_params.yaml",
                "ns": "global_costmap"
              },
              {
                "from": "$(find teb_local_planner_tutorials)/cfg/carlike/costmap_common_params.yaml",
                "ns": "local_costmap"
              },
              {
                "from": "$(find teb_local_planner_tutorials)/cfg/carlike/local_costmap_params.yaml"
              },
              {
                "from": "$(find teb_local_planner_tutorials)/cfg/carlike/global_costmap_params.yaml"
              },
              {
                "from": "$(find teb_local_planner_tutorials)/cfg/carlike/teb_local_planner_params.yaml"
              },
              {
                "name": "base_global_planner",
                "value": "global_planner/GlobalPlanner"
              },
              {
                "name": "planner_frequency",
                "value": "1.0"
              },
              {
                "name": "planner_patience",
                "value": "5.0"
              },
              {
                "name": "base_local_planner",
                "value": "teb_local_planner/TebLocalPlannerROS"
              },
              {
                "name": "controller_frequency",
                "value": "5.0"
              },
              {
                "name": "controller_patience",
                "value": "15.0"
              },
              {
                "name": "clearing_rotation_allowed",
                "value": "false"
              }
            ]
          }
        ]
      }
    }
  }
}